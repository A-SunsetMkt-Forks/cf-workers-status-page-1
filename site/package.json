{
  "name": "cf-status-page-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "yarn dlx @cloudflare/next-on-pages@1",
    "pages:deploy": "pages:build && wrangler pages publish .vercel/output/static",
    "pages:watch": "yarn dlx @cloudflare/next-on-pages@1 --watch",
    "pages:dev": "yarn dlx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.4",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "13.5.6",
    "next": "^14.2.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "server-only": "^0.0.1",
    "typescript": "5.5.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.12.1",
    "@cloudflare/workers-types": "^4.20240718.0",
    "cf-status-page-types": "*",
    "vercel": "^35.1.0"
  }
}
